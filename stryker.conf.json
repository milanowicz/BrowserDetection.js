{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "allowConsoleColors": true,
  "cleanTempDir": true,
  "mutate":[
    "src/**/*.js?(x)"
  ],
  "htmlReporter": {
    "baseDir": "coverage-mutation"
  },
  "packageManager": "yarn",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "dashboard"
  ],
  "testRunner": "jest",
  "coverageAnalysis": "perTest"
}
